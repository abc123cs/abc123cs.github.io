import random
import pandas as pd

# 生成序号、姓名、日期、房间号数据
data = []
for i in range(1, 10001):
    name = ''.join(random.sample('abcdefghijklmnopqrstuvwxyz', 5))
    date = '{}/{}/{}'.format(random.randint(2000, 2022), random.randint(1, 12), random.randint(1, 28))
    room = random.randint(1, 100)
    data.append([i, name, date, room])

# 将数据保存为CSV文件
df = pd.DataFrame(data, columns=['序号', '姓名', '日期', '房间号'])
df.to_csv('data.csv', index=False, encoding='utf-8')
